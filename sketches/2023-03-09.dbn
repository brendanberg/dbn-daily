Set X 34
Set Y 28
Set D 1 // 0 = North, 1 = West, 2 = South, 3 = East
Set S 3

Paper 0

Repeat I 1 512
{
    Set T <Array I>
    Set DD D
    Same? DD 0
    {
        Same? T 0
        {
            Line X Y (X-S) Y
            Set X (X-S)
            Set D 1
        }
        NotSame? T 0
        {
            Line X Y (X+S) Y
            Set X (X+S)
            Set D 3
        }
    }
    Same? DD 1
    {
        Same? T 0
        {
            Line X Y X (Y-S)
            Set Y (Y-S)
            Set D 2
        }
        NotSame? T 0
        {
            Line X Y X (Y+S)
            Set Y (Y+S)
            Set D 0
        }
    }
    Same? DD 2
    {
        Same? T 0
        {
            Line X Y (X+S) Y
            Set X (X+S)
            Set D 3
        }
        NotSame? T 0
        {
            Line X Y (X-S) Y
            Set X (X-S)
            Set D 1
        }
    }
    Same? DD 3
    {
        Same? T 0
        {
            Line X Y X (Y+S)
            Set Y (Y+S)
            Set D 0
        }
        NotSame? T 0
        {
            Line X Y X (Y-S)
            Set Y (Y-S)
            Set D 2
        }
    }
}